{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["../js/mlj","../js/mlj/README.md"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "template": "./MeshLabTemplate",
        "encoding": "utf8",
        "destination": "./html",
        "recurse": true,
        "access": "all"
    }
}